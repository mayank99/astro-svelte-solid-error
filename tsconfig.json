{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"baseUrl": ".",
		"types": ["astro/client"],
		"paths": {
			"~/*": ["src/*"]
		},
		"jsx": "preserve",
		"jsxImportSource": "solid-js"
	}
}
